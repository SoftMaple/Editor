{
  "name": "softmaple",
  "private": true,
  "version": "1.0.0",
  "description": "A Paper Typesetting Editor",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/softmaple"
  },
  "keywords": [
    "latex",
    "reactjs",
    "react-hooks",
    "nextjs",
    "draft-js",
    "rich-text-editor",
    "turborepo"
  ],
  "author": "Yadong Zhang",
  "license": "Apache-2.0",
  "devDependencies": {
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "turbo": "^1.1.1"
  },
  "packageManager": "pnpm@6.28.0"
}
