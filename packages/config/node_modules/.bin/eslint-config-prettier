#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sakuta/Desktop/Next/node_modules/.pnpm/eslint-config-prettier@8.3.0/node_modules/eslint-config-prettier/bin/node_modules:/Users/sakuta/Desktop/Next/node_modules/.pnpm/eslint-config-prettier@8.3.0/node_modules/eslint-config-prettier/node_modules:/Users/sakuta/Desktop/Next/node_modules/.pnpm/eslint-config-prettier@8.3.0/node_modules:/Users/sakuta/Desktop/Next/node_modules/.pnpm/node_modules:/Users/sakuta/Desktop/Next/node_modules:/Users/sakuta/Desktop/node_modules:/Users/sakuta/node_modules:/Users/node_modules:/node_modules:/Users/sakuta/Desktop/Next/packages/config/node_modules:/Users/sakuta/Desktop/Next/packages/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/sakuta/Desktop/Next/node_modules/.pnpm/eslint-config-prettier@8.3.0/node_modules/eslint-config-prettier/bin/node_modules:/Users/sakuta/Desktop/Next/node_modules/.pnpm/eslint-config-prettier@8.3.0/node_modules/eslint-config-prettier/node_modules:/Users/sakuta/Desktop/Next/node_modules/.pnpm/eslint-config-prettier@8.3.0/node_modules:/Users/sakuta/Desktop/Next/node_modules/.pnpm/node_modules:/Users/sakuta/Desktop/Next/node_modules:/Users/sakuta/Desktop/node_modules:/Users/sakuta/node_modules:/Users/node_modules:/node_modules:/Users/sakuta/Desktop/Next/packages/config/node_modules:/Users/sakuta/Desktop/Next/packages/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/eslint-config-prettier@8.3.0/node_modules/eslint-config-prettier/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/eslint-config-prettier@8.3.0/node_modules/eslint-config-prettier/bin/cli.js" "$@"
fi
